[core]
  editor = vim
  excludesfile = /Users/ross/.gitignore_global
[color]
  ui = auto
[alias]
  root = !pwd
  d = diff
  ds = diff --staged
  s = status -s -b
  c = commit -v
  co = checkout
  a = add -u
  ap = add -u -p
  l = log --pretty='format:%C(yellow)%h %C(blue)%ad %C(reset)%s%C(red)%d %C(green)%an%C(reset), %C(cyan)%ar' --date=short
[push]
  default = simple
